<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
  <title>WebSharks Core</title>
  <parser>
    <encoding>utf8</encoding>
    <target>.~build/.~/phpdoc/parser</target>
    <markers>
      <item>TODO</item>
      <item>FIXME</item>
    </markers>
    <extensions>
      <extension>php</extension>
    </extensions>
    <visibility></visibility>
  </parser>
  <transformer>
    <target>.~build/codex</target>
  </transformer>
  <transformations>
    <template name="responsive-twig" />
  </transformations>
  <files>
    <directory>src</directory>
    <ignore>src/vendor/*</ignore>
  </files>
</phpdoc>
