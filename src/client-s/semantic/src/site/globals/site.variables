@headerFont        : inherit;

@h1 : 3.75rem;
@h2 : 2.85rem;
@h3 : 1.875rem;
@h4 : 1.563rem;
@h5 : 1.302rem;
@h6 : 1.302rem;

@headerLineHeight  : 1;
